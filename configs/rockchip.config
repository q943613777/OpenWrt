# Target Config
CONFIG_TARGET_rockchip=y
CONFIG_TARGET_rockchip_armv8=y
CONFIG_TARGET_MULTI_PROFILE=y
# CONFIG_TARGET_DEVICE_rockchip_armv8_DEVICE_fastrhino_r66s is not set
# CONFIG_TARGET_DEVICE_rockchip_armv8_DEVICE_fastrhino_r68s is not set
# CONFIG_TARGET_DEVICE_rockchip_armv8_DEVICE_friendlyarm_nanopi-r2c is not set
CONFIG_TARGET_DEVICE_rockchip_armv8_DEVICE_friendlyarm_nanopi-r2s=y
# CONFIG_TARGET_DEVICE_rockchip_armv8_DEVICE_friendlyarm_nanopi-r4s is not set
# CONFIG_TARGET_DEVICE_rockchip_armv8_DEVICE_friendlyarm_nanopi-r4se is not set
# CONFIG_TARGET_DEVICE_rockchip_armv8_DEVICE_friendlyarm_nanopi-r5s is not set
# CONFIG_TARGET_DEVICE_rockchip_armv8_DEVICE_friendlyarm_nanopi-r5c is not set
# CONFIG_TARGET_DEVICE_rockchip_armv8_DEVICE_hinlink_opc-h66k is not set
# CONFIG_TARGET_DEVICE_rockchip_armv8_DEVICE_hinlink_opc-h68k is not set
# CONFIG_TARGET_DEVICE_rockchip_armv8_DEVICE_hinlink_opc-h69k is not set

# Set Firmware Size
CONFIG_TARGET_KERNEL_PARTSIZE=64
CONFIG_TARGET_ROOTFS_PARTSIZE=960

# Firmware Type
CONFIG_TARGET_ROOTFS_TARGZ=y
# CONFIG_TARGET_ROOTFS_EXT4FS is not set
CONFIG_TARGET_ROOTFS_SQUASHFS=y
